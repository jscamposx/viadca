{
  "name": "demo-viaje",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://viadca.onrender.com",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node env-check.js && vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "commit": "aicommits",
